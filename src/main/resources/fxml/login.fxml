<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="138.0" prefWidth="320" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.airqualitmon.LoginController">
    
    <children>
        <Text layoutX="24.0" layoutY="38.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username:">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Text>
        <Text layoutX="25.0" layoutY="74.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password:">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Text>
        <TextField fx:id="username" layoutX="108.0" layoutY="20.0" prefHeight="25.0" prefWidth="197.0" />
        <PasswordField fx:id="password" layoutX="108.0" layoutY="56.0" prefHeight="25.0" prefWidth="197.0" />
        <Button fx:id="loginButton" layoutX="253.0" layoutY="100.0" mnemonicParsing="false" onAction="#handleLoginAction" text="Login" />
        <Button layoutX="191.0" layoutY="100.0" mnemonicParsing="false" text="Cancel" />
    </children>
</AnchorPane>
